{
  "compilerOptions": {
      "module": "ESNext",
      "outDir": "./public",
      "rootDir": ".",
      "target": "ESNext",
      "declaration": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "noImplicitAny": true,
      "removeComments": false,
      "strict": true,
      "strictBindCallApply": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "baseUrl": "./src",
      "typeRoots": [
        "node_modules/@types"
      ],
      "lib": [
        "esnext",
        "dom"
      ],
      "paths": {
          "@/*": ["./*"]
      }
  },
  "include": [
      "./src/**/*.ts"
  ],
  "exclude": []
}
